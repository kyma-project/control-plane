  "_controlsOrder": [
    "name"
  ],
  "_show_form_view": true,
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "title": "Cluster Name",
      "minLength": 1,
      "pattern": "^[a-zA-Z0-9-]*$",
      "_BTPdefaultTemplate": {
        "elements": [
          "saSubdomain"
        ]
      }
    }
  },
  "required": [
    "name"
  ]
}